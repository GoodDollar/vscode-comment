{
  "name": "addSolidityComments",
  "displayName": "Add solidity comments",
  "description": "Adds solidity comments for all code",
  "icon": "images/comment_logo.svg",
  "galleryBanner": {
    "color": "#5c2d91",
    "theme": "dark"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "bugs": {
    "url": "https://github.com/GoodDollar/vscode-comment/issues"
  },
  "homepage": "https://github.com/GoodDollar/vscode-comment/blob/master/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/GoodDollar/vscode-comment.git"
  },
  "categories": [
    "Other"
  ],
  "version": "0.0.1",
  "publisher": "GoodDollar",
  "engines": {
    "vscode": "0.10.x"
  },
  "main": "./out/extension.js",
  "activationEvents": [
    "onCommand:extension.addSolidityComments"
  ],
  "contributes": {
    "commands": [
      {
        "command": "extension.addSolidityComments",
        "title": "Add Doc Comments"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "@types/node": "^8.10.25",
    "tslint": "^5.11.0",
    "typescript": "^2.6.1",
    "vscode": "^1.1.22"
  },
  "dependencies": {
    "@nodefactory/solidity-comments-core": "^1.2.1"
  }
}
