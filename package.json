{
  "name": "addSolidityComments",
  "displayName": "Add solidity comments",
  "description": "Adds solidity comments for all code",
  "icon": "images/comment_logo.svg",
  "galleryBanner": {
    "color": "#5c2d91",
    "theme": "dark"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "bugs": {
    "url": "https://github.com/GoodDollar/vscode-comment/issues"
  },
  "homepage": "https://github.com/GoodDollar/vscode-comment/blob/master/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/GoodDollar/vscode-comment.git"
  },
  "categories": ["Other"],
  "version": "0.0.1",
  "publisher": "GoodDollar",
  "engines": {
    "vscode": "0.10.x"
  },
  "main": "./out/extension",
  "activationEvents": ["onCommand:extension.addSolidityComments"],
  "contributes": {
    "commands": [
      {
        "command": "extension.addSolidityComments",
        "title": "Add Doc Comments"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
    "compile": "node ./node_modules/vscode/bin/compile -watch -p ./"
  },
  "devDependencies": {
    "vscode": "0.10.0"
  },
  "dependencies": {
    "@nodefactory/solidity-comments-core": "^1.2.1"
  }
}
